    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "env": "^0.0.2",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "guitar",
    "chords",
    "api",
    "express",
    "mongodb"
