{
  "name": "guitar-chords-api",
  "version": "1.0.0",
  "description": "Backend API for Guitar Chords application",
  "main": "bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "seed": "node seedData.js",
    "backup": "node backup.js",
    "restore": "node restore.js",
    "migrate": "node runMigrations.js",
    "migrate:rollback": "node runMigrations.js rollback"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
